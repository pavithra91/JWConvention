    <script>
        $(document).ready(function () {
            $('#example').DataTable({
                dom: 'Bfrtip',
                "pagingType": "full_numbers",
                "lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]],
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });
        });
    </script>

    <script src="~/jquery.js"></script>
