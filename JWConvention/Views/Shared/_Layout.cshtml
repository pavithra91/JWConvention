<body>
    <div class="fix-transparent"></div>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="https://jw2017.org/">
                    <img src="~/images/2018-Final-Logo-1.jpg" class="logo" alt="JW2017">
                </a>
                <img src="~/images/2018-Final-Logo-3.jpg" class="logo-black" alt="Jehovah's Witnesses Special Conventions">
                <div class="top-white">
                    <img src="~/images/2018-Final-Logo-2.jpg" class="logo-white" alt="Jehovah's Witnesses Special Conventions">
                </div>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="http://www.walkerstours.com/">Home</a></li>
                    <li><a href="http://events.walkerstours.net/Tours/TourSummary">Pre & Post Tours</a></li>
                    <li class="dropdown visible-xs">
                </ul>

            </div>
            <div id="is_scrollable" style="color:white;text-align:center;display:none"><b class="caret"></b></div>
        </div>
    </div>
    <div class="fix-overflow">
        <div class="container body-content">
            <div class="shade left"></div>
            <div class="shade right"></div>
            <div class="row">
                <div class="jwcarousel global">
                    <div class="list-jwcarousel" style="width: 8832px;">
                        <ul style="left: -1104px;">
                            <li>
                                <img src="~/images/slider/04.png" data-width="1140" data-height="380" style="width: 100%" alt="Cochabamba" data-cm="cochabamba">
                                <div class="carousel-caption">Sri Lanka<!--cmt═cochabamba--></div>
                            </li>
                            <li>
                                <img src="~/images/slider/01.png" data-width="1140" data-height="380" style="width: 100%" alt="Cochabamba" data-cm="cochabamba">
                                <div class="carousel-caption">Habarana<!--cmt═cochabamba--></div>
                            </li>
                            <li>
                                <img src="~/images/slider/02.png" data-width="1140" data-height="380" style="width: 100%" alt="Jakarta" data-cm="jakarta">
                                <div class="carousel-caption">Colombo<!--cmt═jakarta--></div>
                            </li>
                            <li>
                                <img src="~/images/slider/03.png" data-width="1140" data-height="380" style="width: 100%" alt="Milan" data-cm="milan">
                                <div class="carousel-caption">Minneriya<!--cmt═milan--></div>
                            </li>
                        </ul>
                    </div>
                    <div class="alpha-white-color left"></div>
                    <div class="alpha-white-color right"></div>
                    <a class="carousel-control left" data-slide="prev">
                        <span class="icon-prev" onclick="PrevSlide()">
                            <img src="https://jw.azureedge.net/$root/prevnext.png" alt="Previous" data-cm="cmt═previous">
                        </span>
                    </a>
                    <a class="carousel-control right" data-slide="next">
                        <span class="icon-next" onclick="NextSlide()">
                            <img src="https://jw.azureedge.net/$root/prevnext.png" alt="Next" data-cm="cmt═next">
                        </span>
                    </a>
                </div>
            </div>

            <div>
                @RenderBody()
            </div>
            
            <div class="spacer hidden-print"></div>
            <footer class="hidden-print">
                <div class="copyright">
                    <div class="inside">
                        <p>
                            <a href="https://www.jw.org/finder?docid=1011511&amp;prefer=content" title="Copyright" data-cm="cmt═copyright" target="jworg" style="color:#699c40 !important">Copyright<!--cmt═copyright--></a> © 2018 Walkerstours Sri Lanka.
                        </p>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    @RenderSection("scriptLocation", false)
    @Html.RenderScripts()
</body>
</html>
